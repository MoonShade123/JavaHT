package com.hometask;

public class Car extends HT4A {
 private String kind;

 public String getKind() {
  return kind;
 }

 public void setKind(String kind) {
  this.kind = kind;
 }
 public Car(String name, int capacility, int startYear, int parts, String condition, String  kind) {
  super(name, capacility, startYear, parts, condition, kind);
 }

 @Override
 public String toString() {
  return "Car{" +
          "Kind:" + kind + "}";
 }
}
